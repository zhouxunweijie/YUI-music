.search_group {
  width: 100vw;
  padding: 20rpx 28rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  line-height: 24rpx;
  color: #010101;
}
.search_group .left_icon {
  width: 24rpx;
  height: 32rpx;
  margin-right: 40rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: cover;
}
.search_group .input_group {
  flex: 1;
  background-color: #f3f3f5;
  border-radius: 15rpx;
  height: 54rpx;
  display: flex;
  align-items: center;
  padding: 0 20rpx;
}
.search_group .input_group .input_icon {
  width: 28rpx;
  height: 28rpx;
  margin-right: 12rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: cover;
}
.search_group .right_text {
  margin-left: 40rpx;
}

