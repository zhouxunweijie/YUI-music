.appList {
  display: flex;
}
.appList .appList_item {
  width: 25%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.appList .appList_item .image {
  width: 72rpx;
  height: 72rpx;
}
.appList .appList_item .text {
  font-size: 18rpx;
  color: #bfbfbf;
  margin-top: 20rpx;
}
.appList .appList_item .text.active {
  color: #010101;
}
.list {
  height: calc(100vh - 240rpx - 25px);
  overflow-y: auto;
  width: 100vw;
}
.list .list_item {
  padding: 30rpx 28rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.list .list_item_add {
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  width: 30rpx;
  height: 30rpx;
  margin-right: 28rpx;
}
.list .list_item_content {
  flex: 1;
  font-size: 26rpx;
  color: #009ae7;
  line-height: 26rpx;
  display: flex;
  flex-direction: column;
}
.list .list_item_content .artist {
  font-size: 20rpx;
  color: #787b83;
  margin-top: 12rpx;
}
.list .list_item_utils {
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  width: 10rpx;
  height: 18rpx;
}

