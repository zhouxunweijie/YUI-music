.scrollGroup {
  height: 100vh;
  width: 100vw;
  overflow-y: auto;
  box-sizing: border-box;
}
.title_group {
  position: relative;
}
.title_group .bg_img {
  position: absolute;
  display: block;
  width: 100vw;
  height: 600rpx;
  background-size: 600% 600%;
  background-repeat: no-repeat;
  -webkit-filter: blur(260rpx);
          filter: blur(260rpx);
  z-index: -1;
}
.title_group .back {
  font-size: 32rpx;
  font-weight: 600;
  display: flex;
  align-items: center;
  padding: 20rpx 28rpx;
  color: #000000;
  position: fixed;
  top: 25px;
  width: 100vw;
  z-index: 100;
  transition: all 0.5s;
}
.title_group .back .left_icon {
  width: 24rpx;
  height: 32rpx;
  margin-right: 28rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: cover;
}
.title_group .details {
  padding: 50rpx 35rpx;
  display: flex;
  align-items: flex-start;
  margin-top: 60rpx;
}
.title_group .details .img {
  width: 212rpx;
  height: 212rpx;
}
.title_group .details .footer {
  max-width: calc(100vw - 300rpx);
  margin-left: 28rpx;
  flex: 1;
}
.title_group .details .footer .name {
  font-size: 30rpx;
  font-weight: 600;
  display: block;
  color: #000000;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.title_group .details .footer .desc {
  display: block;
  margin-top: 40rpx;
  font-size: 24rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 4;
  line-clamp: 4;
  -webkit-box-orient: vertical;
}
.title_group .details .footer .sc {
  margin-top: 30rpx;
  display: inline-block;
  font-size: 22rpx;
  color: #000000;
  padding: 20rpx 30rpx;
  border-radius: 15rpx;
  background-color: #dbddda;
  text-align: center;
  line-height: 22rpx;
}
.list_group {
  position: absolute;
  top: 500rpx;
  left: 0;
  background-color: #ffffff;
  border-radius: 15px 15px 0 0;
  width: 100vw;
}
.list_group.fiexd {
  position: fixed;
  top: 0;
  overflow-y: auto;
  height: calc(100vh - 25px);
}
.list {
  padding-bottom: 110rpx;
}
.list .list_item {
  padding: 30rpx 28rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.list .list_item_add {
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  width: 30rpx;
  height: 30rpx;
  margin-right: 28rpx;
}
.list .list_item_img {
  width: 63rpx;
  height: 63rpx;
  border-radius: 10rpx;
  margin-right: 20rpx;
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.list .list_item_content {
  flex: 1;
  font-size: 26rpx;
  color: #009ae7;
  line-height: 26rpx;
  display: flex;
  flex-direction: column;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.list .list_item_content .artist {
  font-size: 20rpx;
  color: #787b83;
  margin-top: 12rpx;
}
.list .list_item_utils {
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  background-repeat: no-repeat;
  width: 10rpx;
  height: 18rpx;
}

